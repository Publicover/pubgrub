<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<p><%= link_to 'Back to dashboard', root_path %></p>

<table>
  <thead>
    <th>
      First Name
    </th>
    <th>
      Email
    </th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td>
        <%= link_to "#{user.fname}", user_path(user) %>
      </td>
      <td>
        <%= user.email %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
